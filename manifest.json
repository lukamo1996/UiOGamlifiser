    "name": "UiO Gamlifiser",
    "description": "For de som likte det gamle UiO designet. Woohoo. Lagd av Luka Momcilovic",
    "version": "3.6",
    "manifest_version": 2,
    "browser_action": {
      "default_title":"Dale a tu cuerpo alegr√≠a, Macarena...",
      "default_popup": "popup.html"
    },
    "background":{
      "scripts": ["background.js"],
      "persistent": true
    },
    "permissions":["webRequestBlocking", "webRequest", "*://*.uio.no/*", "storage"],
    "icons": { 
      "128": "ikonON.png" 
    },
    "web_accessible_resources": [
      "style.css"
    ]
}